<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    android:orientation="vertical"
    tools:context="com.wbteam.YYzhiyue.ui.mine.MineHomeFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dip"
        android:visibility="gone"
        android:background="@color/bg_color">


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dip"
                android:layout_marginTop="5dip"
                android:background="@drawable/shape_round_group_btn_bg">

                <com.wbteam.YYzhiyue.view.MyGridView
                    android:id="@+id/RecyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dip"
                    android:layout_marginRight="10dip"
                    android:layout_marginTop="30dip"
                    android:numColumns="4"></com.wbteam.YYzhiyue.view.MyGridView>

            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dip">

                <TextView
                    android:id="@+id/tv01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="10dip"
                    android:layout_marginStart="9dp"
                    android:background="@drawable/shape_round_group_btn_bg03"
                    android:paddingBottom="4dip"
                    android:paddingLeft="8dip"
                    android:paddingRight="8dip"
                    android:paddingTop="4dip"
                    android:text="出租范围"
                    android:textColor="@color/white" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dip"
                    android:layout_marginTop="8dip"
                    android:layout_toRightOf="@+id/tv01"
                    android:text="审核中"
                    android:textColor="@color/font_color1"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_scopeEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dip"
                    android:layout_marginTop="5dip"
                    android:background="@color/font_color1"
                    android:paddingBottom="2dip"
                    android:paddingLeft="8dip"
                    android:paddingRight="8dip"
                    android:paddingTop="2dip"
                    android:text="编辑"
                    android:textColor="@color/white"
                    android:textSize="11sp" />
            </RelativeLayout>

        </FrameLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dip"
        android:background="@color/bg_color">


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dip"
                android:background="@drawable/shape_round_group_btn_bg"
                android:orientation="vertical"
                android:paddingBottom="10dip">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dip"
                    android:layout_marginTop="30dip"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/tv_constellation01"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="星座:"
                            android:textColor="@color/font_color"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_constellation"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:layout_toRightOf="@+id/tv_constellation01"
                            android:textSize="12sp" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/tv_pricing01"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="定价:"
                            android:textColor="@color/font_color"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_pricing"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:layout_toRightOf="@+id/tv_pricing01"
                            android:textSize="12sp" />

                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dip"
                    android:layout_marginTop="5dip"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/tv_job01"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="职业:"
                            android:textColor="@color/font_color"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_job"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:layout_toRightOf="@+id/tv_job01"
                            android:textSize="12sp" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/tv_diaries01"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="档期:"
                            android:textColor="@color/font_color"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_diaries"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:layout_toRightOf="@+id/tv_diaries01"
                            android:textSize="12sp" />

                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dip"
                    android:layout_marginTop="5dip"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/tv_height01"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="身高:"
                            android:textColor="@color/font_color"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_height"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:layout_toRightOf="@+id/tv_height01"
                            android:textSize="12sp" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/tv_weixin01"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="微信号:"
                            android:textColor="@color/font_color"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_weixin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:layout_toRightOf="@+id/tv_weixin01"
                            android:textSize="12sp" />

                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dip"
                    android:layout_marginTop="5dip"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="手机号:"
                        android:textColor="@color/font_color"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/tv_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dip"
                        android:textSize="12sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dip"
                    android:layout_marginTop="5dip"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="签名:"
                        android:textColor="@color/font_color"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/tv_signature"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dip"
                        android:textSize="12sp" />

                </LinearLayout>
            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dip">

                <TextView
                    android:id="@+id/tv02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="10dip"
                    android:layout_marginStart="9dp"
                    android:background="@drawable/shape_round_group_btn_bg05"
                    android:paddingBottom="4dip"
                    android:paddingLeft="8dip"
                    android:paddingRight="8dip"
                    android:paddingTop="4dip"
                    android:text="个人资料"
                    android:textColor="@color/white" />

                <TextView
                    android:id="@+id/tv_informationEdit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dip"
                    android:layout_marginTop="5dip"
                    android:background="@color/font_color1"
                    android:paddingBottom="2dip"
                    android:paddingLeft="8dip"
                    android:paddingRight="8dip"
                    android:paddingTop="2dip"
                    android:text="编辑"
                    android:textColor="@color/white"
                    android:textSize="14sp" />
            </RelativeLayout>

        </FrameLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dip"
        android:background="@color/bg_color">


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dip"
                android:layout_marginTop="5dip"
                android:background="@drawable/shape_round_group_btn_bg">

                <com.wbteam.YYzhiyue.view.custom_scrollview.MyRecyclerView
                    android:id="@+id/RecyclerView01"
                    android:layout_width="match_parent"
                    android:layout_height="200dip"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="10dip"
                    android:layout_marginRight="10dip"
                    android:layout_marginTop="30dip"
                    android:gravity="center_horizontal"></com.wbteam.YYzhiyue.view.custom_scrollview.MyRecyclerView>

                <!--  <android.support.v7.widget.RecyclerView
                      android:id="@+id/RecyclerView01"
                      android:layout_width="match_parent"
                      android:layout_height="200dip"
                      android:layout_centerInParent="true"
                      android:layout_marginLeft="10dip"
                      android:layout_marginRight="10dip"
                      android:layout_marginTop="30dip"
                      android:gravity="center_horizontal"
                      android:background="@color/bg_color"></android.support.v7.widget.RecyclerView>-->
            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dip">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="10dip"
                    android:layout_marginStart="9dp"
                    android:background="@drawable/shape_round_group_btn_bg07"
                    android:paddingBottom="4dip"
                    android:paddingLeft="8dip"
                    android:paddingRight="8dip"
                    android:paddingTop="4dip"
                    android:text="个人动态"
                    android:textColor="@color/white" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dip"
                    android:layout_marginTop="5dip"
                    android:paddingBottom="2dip"
                    android:paddingLeft="8dip"
                    android:paddingRight="8dip"
                    android:paddingTop="2dip"
                    android:text="动态列表"
                    android:textColor="@color/font_color"
                    android:textSize="14sp" />
            </RelativeLayout>

        </FrameLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dip"
        android:background="@color/bg_color">


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dip"
                android:layout_marginTop="5dip"
                android:background="@drawable/shape_round_group_btn_bg">

                <com.wbteam.YYzhiyue.view.custom_scrollview.MyRecyclerView
                    android:id="@+id/RecyclerView02"
                    android:layout_width="match_parent"
                    android:layout_height="200dip"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="10dip"
                    android:layout_marginRight="10dip"
                    android:layout_marginTop="30dip"
                    android:gravity="center_horizontal"></com.wbteam.YYzhiyue.view.custom_scrollview.MyRecyclerView>

                <!--  <android.support.v7.widget.RecyclerView
                      android:id="@+id/RecyclerView01"
                      android:layout_width="match_parent"
                      android:layout_height="200dip"
                      android:layout_centerInParent="true"
                      android:layout_marginLeft="10dip"
                      android:layout_marginRight="10dip"
                      android:layout_marginTop="30dip"
                      android:gravity="center_horizontal"
                      android:background="@color/bg_color"></android.support.v7.widget.RecyclerView>-->
            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dip">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="10dip"
                    android:layout_marginStart="9dp"
                    android:background="@drawable/shape_round_group_btn_bg04"
                    android:paddingBottom="4dip"
                    android:paddingLeft="8dip"
                    android:paddingRight="8dip"
                    android:paddingTop="4dip"
                    android:text="个人相册"
                    android:textColor="@color/white" />


                <TextView
                    android:id="@+id/tv_move_photo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dip"
                    android:layout_marginTop="5dip"
                    android:paddingBottom="2dip"
                    android:paddingLeft="8dip"
                    android:paddingRight="8dip"
                    android:paddingTop="2dip"
                    android:text="相册列表"
                    android:textColor="@color/font_color"
                    android:textSize="14sp" />
            </RelativeLayout>

        </FrameLayout>

    </RelativeLayout>
</LinearLayout>
